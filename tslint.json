{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": [
      true,
      140
    ],
    "no-console": false,
    "no-unused-expression": false,
    "no-empty": false,
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": []
}
